{
  "name": "ember-cognito",
  "version": "0.12.0",
  "description": "ember-simple-auth and AWS Cognito integration",
  "keywords": [
    "ember-addon",
    "cognito"
  ],
  "repository": "https://github.com/paulcwatts/ember-cognito",
  "license": "MIT",
  "author": "Paul Watts <paulcwatts@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@aws-amplify/auth": "^3.4.15",
    "@aws-amplify/core": "^3.8.7",
    "amazon-cognito-identity-js": "^4.5.5",
    "ember-auto-import": "^2.3.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.22.0",
    "ember-cli-code-coverage": "^1.0.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator-for-testing": "^1.0.0",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-simple-auth": "^3.0.1",
    "ember-sinon": "^5.0.0",
    "ember-source": "~3.23.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-disable-features": "^0.1.3",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.5.0",
    "release-it": "^14.2.2",
    "webpack": "^5.66.0"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": true
    }
  },
  "engines": {
    "node": ">= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "greenkeeper": {
    "ignore": [
      "ember-cli",
      "ember-cli-babel",
      "ember-cli-dependency-checker",
      "ember-cli-htmlbars",
      "ember-cli-htmlbars-inline-precompile",
      "loader.js"
    ]
  },
  "volta": {
    "node": "12.13.0",
    "yarn": "1.22.17"
  }
}
